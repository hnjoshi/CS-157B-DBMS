<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="edu.cs157b.hibernate.Doctor" table="Doctor">
		<id name="doctorId" column="doctorId" type="integer">
		<generator class="increment"></generator>
		</id>
		<property name="name" column="doctor_name" type="string"></property>
		<property name="speciality" column="speciality" type="string"></property>
		
	</class>
</hibernate-mapping>